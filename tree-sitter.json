{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "abc",
      "camelcase": "Abc",
      "title": "Abc",
      "scope": "source.abc",
      "file-types": [
        "abc"
      ],
      "injection-regex": "^abc$",
      "class-name": "TreeSitterAbc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Abc grammar for tree-sitter",
    "authors": [
      {
        "name": "mt"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-abc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}